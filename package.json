{
  "name": "webpack_project",
  "version": "1.0.0",
  "description": "webpack project front end course Beetroot Academy",
  "main": "index.js",
  "scripts": {
    "tailwind": "tailwindcss -i ./src/tailwind.css -o ./src/scss/_tailwind.scss --watch",
    "prettier": "npx prettier src/index.html --write",
    "webpack": "webpack",
    "start": "webpack serve --mode=development",
    "production": "npm run webpack -- --mode=production",
    "browser": "browser-sync start  -s \"public\"  -w    -f=\"./index.html\" --no-notify",
    "build": "npm run production && npm run browser",
    "sync": "browser-sync start --server --files \"**/*\"",
    "dev": "postcss tailwind.css --output output.css",
    "watch:dev": "nodemon -x npm run dev -w tailwind.config.js -w tailwind.css",
    "watch": "run-p watch:dev sync"
  },
  "keywords": [
    "Beetroot",
    "Academy",
    "Front-end"
  ],
  "author": "Roman Nikiforov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.29.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "daisyui": "^4.9.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^9.0.0",
    "gulp-postcss": "^10.0.0",
    "html-webpack-plugin": "^5.5.3",
    "live-server": "^1.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swiper": "^11.1.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.20.0",
    "terser-webpack-plugin": "^5.3.9",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-concat-files-plugin": "^0.5.2",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge-and-include-globally": "^2.3.4"
  },
  "dependencies": {
    "gsap": "^3.12.5"
  }
}
